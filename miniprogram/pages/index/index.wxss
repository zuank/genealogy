/* miniprogram/pages/index/index.wxss */
page {
  width: 100%;
  height: 100%;
}
.container {
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.genealogy-list {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.genealogy-item {
  flex: 1;
  width: 400rpx;
  margin: 40rpx;
  border-radius: 20rpx;
  border: 1rpx solid #cccccc;
  display: flex;
  align-items: center;
  justify-content: center;
}

.new-genealogy {
  margin: 20rpx;
}